{
  "name": "sportport-active",
  "version": "1.0.0",
  "description": "Child theme for SportPort Active built on the Genesis framework",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/joedooley/sportport-active.git"
  },
  "author": "Joe Dooley <hello@developingdesigns.com> (https://www.developingdesigns.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joedooley/sportport-active/issues"
  },
  "homepage": "https://github.com/joedooley/sportport-active",
  "scripts": {
    "scss": "node-sass --source-map true -w assets/sass -o ./",
    "autoprefixer": "postcss -u autoprefixer -r style.css",
    "eslint": "eslint assets/js",
    "babel": "babel assets/js/** -d dist/js -s",
    "serve": "browser-sync start --ws --proxy http://spa.dev --logLevel 'info' --no-ghost-mode --no-notify --files 'style.css, !style.css.map,!assets/sass/**,js!assets/js/**'",
    "build:css": "npm run scss && npm run autoprefixer",
    "build:js": "npm run eslint && npm run uglify",
    "build:all": "npm run build:css && npm run build:js",
    "watch:css": "onchange 'assets/sass/style.scss' -- npm run build:css",
    "watch:js": "onchange 'assets/js/*.js' -- npm run build:js",
    "watch:all": "npm-run-all scss serve"
  },
  "dependencies": {
    "bourbon": "^4.2.7",
    "bourbon-neat": "^1.8.0",
    "normalize.css": "^5.0.0",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^2.1.5",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.18.0",
    "browser-sync": "^2.17.5",
    "eslint": "^3.8.1",
    "flickity": "^1.2.1",
    "node-sass": "^3.10.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "onchange": "^3.0.2",
    "postcss-cli": "^2.6.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "sass-lint": "^1.9.1",
    "uglify-js": "^2.7.4"
  }
}
